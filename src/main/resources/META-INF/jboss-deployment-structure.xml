<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
    <deployment>
        <!-- Exposes the jboss-modules from $KEYCLOAK_HOME/modules for use
        of this extension jar. This is a generous collection of dependencies
        to simplify the development of extensions. -->
        <dependencies>
            <module name="org.keycloak.keycloak-common"/>
            <module name="org.keycloak.keycloak-core"/>
            <module name="org.keycloak.keycloak-server-spi"/>
            <module name="org.keycloak.keycloak-server-spi-private"/>
            <module name="org.jboss.resteasy.resteasy-jaxrs"/>
            <module name="org.apache.httpcomponents"/>
            <module name="org.keycloak.keycloak-services"/>
            <module name="org.keycloak.keycloak-model-infinispan" export="true"/>
            <module name="io.smallrye.metrics" export="true"/>
            <module name="org.eclipse.microprofile.metrics.api" export="true"/>
            <module name="org.bouncycastle"/>
            <module name="com.google.guava"/>
            <module name="org.jboss.logging"/>
            <module name="org.apache.commons.io"/>
            <module name="javax.api"/>
            <module name="javax.transaction.api"/>
            <module name="javax.enterprise.api"/>
            <module name="org.freemarker"/>
            <module name="com.googlecode.owasp-java-html-sanitizer"/>
            <module name="com.fasterxml.jackson.core.jackson-core"/>
            <module name="com.fasterxml.jackson.core.jackson-annotations"/>
            <module name="com.fasterxml.jackson.core.jackson-databind"/>
            <module name="com.google.zxing.core"/>
            <module name="com.google.zxing.javase"/>
            <module name="org.infinispan"/>
            <module name="org.infinispan.commons"/>
            <module name="org.infinispan.persistence.remote"/>
            <module name="org.infinispan.client.hotrod"/>
            <module name="org.infinispan.jboss-marshalling"/>
        </dependencies>
    </deployment>
</jboss-deployment-structure>
